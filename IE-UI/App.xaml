<Application x:Class="IE_UI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:IE_UI"
             StartupUri="Views\MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="ForegroundThemeBrush" Color="#FF7400"/>
        <SolidColorBrush x:Key="ForegroundAccentBrush" Color="#FF943C"/>
        <SolidColorBrush x:Key="BackgroundThemeBrush" Color="#F5F1EE"/>
        <SolidColorBrush x:Key="BackgroundLightBrush" Color="#F9E3D1"/>
        <SolidColorBrush x:Key="BackgroundHeavyBrush" Color="#FFC290"/>
    </Application.Resources>
</Application>
